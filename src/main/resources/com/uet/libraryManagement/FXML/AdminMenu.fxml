         <ImageView fx:id="DL_image" fitHeight="20.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
            <Image url="@../ICONS/dark.png" />
         </ImageView>
      </graphic>
   </JFXButton>
   <JFXButton contentDisplay="GRAPHIC_ONLY" focusTraversable="false" layoutY="3.0" onAction="#menuButton" prefHeight="40.0" prefWidth="187.0" styleClass="UserMenu-JFXButton-menuButton" stylesheets="@../CSS/Light-mode.css">
      <graphic>
         <ImageView fitHeight="30.0" fitWidth="40.0" pickOnBounds="true">
            <Image url="@../ICONS/menu.png" />
         </ImageView>
      </graphic>
      <font>
         <Font size="16.0" />
      </font>
   </JFXButton>
</AnchorPane>
