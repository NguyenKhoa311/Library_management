            <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../ICONS/manage.png" />
               </image>
            </ImageView>
         </graphic>
      </JFXButton>
      <JFXButton alignment="BASELINE_LEFT" focusTraversable="false" onAction="#Documents" prefHeight="50.0" prefWidth="156.0" styleClass="UserMenu-VBox" stylesheets="@../CSS/Light-mode.css" text="Documents">
         <graphic>
            <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
               <Image url="@../ICONS/documents.png" />
            </ImageView>
         </graphic>
      </JFXButton>
      <JFXButton alignment="BASELINE_LEFT" focusTraversable="false" onAction="#SearchDocuments" prefHeight="50.0" prefWidth="158.0" styleClass="UserMenu-VBox" stylesheets="@../CSS/Light-mode.css" text="Search documents">
         <graphic>
            <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
               <Image url="@../ICONS/search_document.png" />
            </ImageView>
         </graphic>
      </JFXButton>
      <JFXButton alignment="BASELINE_LEFT" focusTraversable="false" onAction="#LogOut" prefHeight="50.0" prefWidth="158.0" styleClass="UserMenu-VBox" stylesheets="@../CSS/Light-mode.css" text="Log out">
         <graphic>
            <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
               <Image url="@../ICONS/exit.png" />
            </ImageView>
         </graphic>
      </JFXButton>
      <padding>
         <Insets top="5.0" />
      </padding>
   </VBox>
   <Label fx:id="Title_page" layoutX="-8.0" layoutY="-6.0" prefHeight="50.0" prefWidth="710.0" styleClass="UserMenu-Label-Title_page" stylesheets="@../CSS/Light-mode.css" text="Title" textFill="#8eccf5" />
